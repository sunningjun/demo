.search {
  display: flex;
  width: 90%;
  border: solid 1px whitesmoke;
  border-radius: 5px;
  background-color: ghostwhite;
  margin-left: 5%;
  height: 20px;
}

.search-image {
  width: 20px;
  height: 10%;
  margin-right: 10px;
}

.search .stuff {
  font-size: 12px;
}

.swipper {
  width: 100%;
  margin: 10px 0px 0px 0px;
  height: 40%;
}

.details {
  width: 100%;
  height: 50%;
}


 .slide-image{
  width: 100%;
} 

.title {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  margin-top: 5px;
  font-size: 16px;
}

.title_selected {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
  margin-top: 5px;
  font-size: 16px;
  background-color: gainsboro;
}

.title_image {
  width: 15px;
  height: 15px;
}

.more {
  color: gainsboro;
}

.WScrollView1 {
  box-sizing: border-box;
  white-space: nowrap;
  display: flex;
  margin: 8PX 0PX 0PX 0PX;
  width: 100%;
  height: 200PX;
  background-color: #fff;
  color: #fff;
  font-size: 20PX;
  line-height: 200PX;
}

.WViewRow1 {
  box-sizing: border-box;
  display: inline-block;
  justify-content: flex-start;
  align-items: flex-start;
  flex-direction: row;
  flex-wrap: wrap;
  align-content: stretch;
  width: 200px;
  height: 350px;
  margin-right: 10px;
  background-color: #fff;
  color: #fff;
  font-size: 20PX;
  line-height: 20PX;
  border-radius: 2px;
}

.WImage {
  width: 200px;
  height: 150px;
  border-radius: 2px;
}

.WViewRow2 {
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: flex-start;
  flex-direction: column;
  flex-wrap: wrap;
  width: 200PX;
  background-color: #fff;
  color: #fff;
  font-size: 20PX;
  line-height: 40PX;
  border: medium solid whitesmoke;
  border-radius: 5px;
  border-top: 0px;
}

.WText16PT1 {
  background-color: transparent;
  color: #000;
  font-size: 14PX;
  line-height: 16PX;
}

.view_fiexd {
  position: fixed;
  bottom: 5%;
  right: 5px;
  height: 120px;
}

.view_fiexd_item {
  height: 30%;
  width: 30px;
  background-color: gray;
  opacity: 0.8;
  margin-bottom: 5%;
}

.view_fiexd_item image {
  width: 30px;
  height: 30px;
}
