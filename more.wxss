.container{
  box-sizing: border-box;
  display: inline-block;
  background-color: whitesmoke;
  width: 100%;
  height: 100%;
}
.WViewRow1{
 box-sizing:border-box;
 display: inline-block; 
 justify-content:flex-start;
 align-items:flex-start;
 flex-direction:row;
 flex-wrap:wrap;
 align-content:stretch;
 width:45%; 
 height: 200px;
 /* margin: 10px 10px 10px 10px; */
  margin:2% 2% 2% 2%;  
  /* margin-bottom: 2px;  */
